{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/dashboard/(.*)",
      "destination": "/dashboard/$1"
    },
    {
      "source": "/_next/(.*)",
      "destination": "/_next/$1"
    },
    {
      "source": "/(?!api|dashboard|_next)([^.]+)",
      "destination": "/api/links/$1"
    }
  ]
}